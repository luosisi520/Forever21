    <script type="text/javascript">
        //slider 20160620 추가
        $(document).ready(function () {
            var arrParent = [{ obj: $("#owl-main-one"), idx: 0 }
                            ,{ obj: $("#owl-main-two"), idx: 0 }
                            ,{ obj: $("#owl-main-three"), idx: 0}];

            $("#owl-top-banner").owlCarousel({
                navigation: true, // Show next and prev buttons
                slideSpeed: 300,
                paginationSpeed: 400,
                autoPlay: false,
                navigationText: ["<", ">"],
                rewindNav: false,
                singleItem: true
            });

            $("#owl-main-one").owlCarousel({
                navigation: true, // Show next and prev buttons
                slideSpeed: 300,
                pagination: false,
                paginationSpeed: 400,
                autoPlay: false,
                navigationText: ["<", ">"],
                rewindNav: false,
                singleItem: true
            });

            $("#owl-main-two").owlCarousel({
                navigation: true, // Show next and prev buttons
                slideSpeed: 300,
                pagination: false,
                paginationSpeed: 400,
                autoPlay: false,
                navigationText: ["<", ">"],
                rewindNav: false,
                singleItem: true
            });

            $("#owl-main-three").owlCarousel({
                navigation: true, // Show next and prev buttons
                slideSpeed: 300,
                pagination: false,
                paginationSpeed: 400,
                autoPlay: false,
                navigationText: ["<", ">"],
                rewindNav: false,
                singleItem: true
            });


            $('.owl-prev').unbind('click');
            $('.owl-next').unbind('click');

            $('.owl-prev').click(function () {
                var curr = $(this);
                $.each(arrParent, function (i, item) {
                    if (curr.parents().is(item.obj)) {
                        //parent = $(item[i].name);
                        item.idx = item.idx - 1;
                        if (item.idx < 0)
                            item.idx = $(item.obj).data('owlCarousel').itemsAmount;
                        item.obj.data('owlCarousel').goTo(item.idx);
                        return;
                    }
                });
            });

            $('.owl-next').click(function () {
                var curr = $(this);
                $.each(arrParent, function (i, item) {
                    if (curr.parents().is(item.obj)) {
                        //parent = $(item[i].name);
                        item.idx = item.idx + 1;
                        if (item.idx >= $(item.obj).data('owlCarousel').itemsAmount)
                            item.idx = 0;
                        item.obj.data('owlCarousel').goTo(item.idx);
                        return;
                    }
                });
            });

        });
    </script>

