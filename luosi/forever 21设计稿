        function fnShowVideoPopUp(video, src) {
            if (src == "youtube") {
                var video_content = '<div class="video-container"><iframe width="100%" height="500" src="https://www.youtube.com/embed/' + video + '?rel=0&autoplay=1" frameborder="0" allowfullscreen></iframe></div>'
            }
            else {
                var video_content = '<div style="margin:-10px -5px;"><video width="100%" autoplay controls onended="loopVideo(this)"><source src="http://www.forever21.com/images/video/' + src + '/' + video + '.mp4" type="video/mp4"></video><div>'
            }

            $.confirm({
                title: '',
                content: video_content,
                animation: 'top',
                closeIcon: true,
                confirmButton: false,
                cancelButton: false,
                columnClass: 'col-md-9 video_pop',
                onOpen: function () {
                    //$('.closeIcon').css({ 'display': 'none' });
                    if (src == "youtube") {
                        $('.jconfirm .jconfirm-box div.content').css({ 'padding': '2em' });
                    }
                    else {
                        $('.jconfirm .jconfirm-box div.content').css({ 'padding': '2.5em' });
                    }
                    $('.jconfirm .jconfirm-box div.closeIcon').css({ 'top': '1%' }).css({ 'right': '1%' });
                }
            });
        }  

        function getCookie(cname) {
            var name = cname + "=";

            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }

        function getCookieNew(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
        }

        function checkCookieName(cname) {
            var user = getCookieNew(cname);
            return user;
        }

        function checkCookieValue(cname, pname) {
            var user = getCookieNew(cname);
            var name = pname + "=";
            var ca = user.split('&');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            } 
            return "";
        }

        function checkloginlink() {
            var basketCount = checkCookieValue("UserTrace", "basketCount");
            if (basketCount != '0' && basketCount != '') {
                $("#ctl00_haveBasket").addClass('no_shoppingbag');
                $("#ctl00_haveBasket").css('display', 'block');
                $('#ctl00_haveBasket').html(basketCount);
            }
            else {
                $("#ctl00_haveBasket").removeClass('no_shoppingbag');
                $("#ctl00_haveBasket").css('display', 'none');
            }

            if (checkCookieName("LoggedIn") != "1") {
                $('#loginlink').css('display', 'block');
                $('#loginlink span.top_text').html('登录 / 注册');
                $('#myaccountlink').css('display', 'none');
                $('#divMyAccountList').css('display', 'none');
            }
            else {
                var url = '/Ajax/Ajax_Login.aspx?method=GETUSERNAME';

                $.ajax({
                    type: "GET",
                    url: url,
                    success: function (result) {
                        if (result == '') {
                            $('#loginlink').css('display', 'block');
                            $('#loginlink span.top_text').html('登录 / 注册');
                            $('#myaccountlink').css('display', 'none');
                            $('#divMyAccountList').css('display', 'none');
                        }
                        else {
                            loggedUserName = result;

                            $('#loginlink').css('display', 'block');
                            $('#loginlink span.top_text').html('你好, ' + loggedUserName);
                            $('#myaccountlink').css('display', 'none');
                        }
                    }
                });
            }
        }

        function checkLastCategoryPage() {
            var referrer = document.referrer;
            var href = window.location.href;
            if (href.toLowerCase().indexOf("category.aspx") < 0 && href.toLowerCase().indexOf("landing.aspx") < 0 && href.toLowerCase().indexOf("lookbooks/") < 0 && href.toLowerCase().indexOf("sale/") < 0 && href.toLowerCase().indexOf("holidayshop/") < 0) {
                if (referrer.toLowerCase().indexOf("category.aspx") > 0 || referrer.toLowerCase().indexOf("landing.aspx") > 0 || referrer.toLowerCase().indexOf("lookbooks/") > 0 || referrer.toLowerCase().indexOf("sale/") > 0 || referrer.toLowerCase().indexOf("holidayshop/") > 0) {
                    //Last category page save in Cookie.
                    setCookie("lastcategory", referrer, 365);
                }
            }
        }

        function continueShoppingUrl() {
            var lastCategoryUrl = getCookieNew("lastcategory");
            if (lastCategoryUrl == "") {
                window.location = "/Product/Category.aspx?br=f21&category=whatsnew_all";
            }
            else {
                window.location = lastCategoryUrl;
            }
        }

        function fnToggleMegaSubMenu(subDivId) {
            if ($('#' + subDivId).css('display') == 'block')
                $('#' + subDivId).css('display', 'none');
            else
                $('#' + subDivId).css('display', 'block');
        }

        Number.prototype.formatMoney = function (places, symbol, thousand, decimal) {
            places = !isNaN(places = Math.abs(places)) ? places : 2;
            symbol = symbol !== undefined ? symbol : "$";
            thousand = thousand || ",";
            decimal = decimal || ".";
            var number = this,
	    negative = number < 0 ? "-" : "",
	    i = parseInt(number = Math.abs(+number || 0).toFixed(places), 10) + "",
	    j = (j = i.length) > 3 ? j % 3 : 0;
            return symbol + negative + (j ? i.substr(0, j) + thousand : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thousand) + (places ? decimal + Math.abs(number - i).toFixed(places).slice(2) : "");
        };

