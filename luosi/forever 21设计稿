<script type="text/javascript">
    var countdownTimer;

    function fnHideTooltip() {
        $('div.tooltip').css('display', 'none');
    }
    
    function fnChangeProductImageForMouseEvent(image, imageFilePath) {
        var imageTest = new Image();
        imageTest.onload = function () {
            setTimeout(function () { image.src = imageFilePath }, 100);
        };
        imageTest.onerror = function () {
            //alert('error loading');
        };
        imageTest.src = imageFilePath;
    }


    function imgError(image) {
        image.onerror = "";
        image.src = "http://www.forever21.cn/images/no_image.jpg";
        //image.src = originalImage;
        return true;
    }

