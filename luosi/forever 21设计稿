        function fnGetBasketSubTotal() {
            var url = "/Ajax/AjaxBasketUpdate.aspx?action=status";
            $.ajax({
                type: "POST",
                url: url,
                contentType: "application/json; charset=euc-kr",
                dataType: "json",
                success: function (response) {
                    $("#" + haveTotalID).html(response[0].SubTotal);
                    $("#" + haveBasketID).html(response[1].Qty);
                },
                error: function (msg) {

                }
            });
        }

