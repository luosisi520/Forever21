    <script type="text/javascript">

        function Blink() {
            obj = document.getElementById("blink");
            if (obj.style.visibility == "hidden") obj.style.visibility = "visible";
            else obj.style.visibility = "hidden";
            window.setTimeout("Blink();", 500);
        }
    </script>

    <script type="text/javascript">

        $(document).ready(function () {
            if ($("#popUp").length) {

                $('#popUpBody').click(function (e) {
                    e.stopPropagation();
                    return false;
                });
            }

            if ($("#popUp_marketing").length) {
                $('#popUp_marketing').live('click', function (e) {
                    showPopup();
                });

                $('#popUpBody_marketing').click(function (e) {
                    e.stopPropagation();
                    return false;
                });
            }

            if (window.location.pathname == '/' || window.location.pathname.toLowerCase().indexOf('/product/main.aspx') == 0 || window.location.pathname.toLowerCase().indexOf('/default.aspx') == 0) {
                $('div#main').addClass('homepage');
            }

            if (window.location.pathname.toLowerCase().indexOf('/product/category.aspx') == 0) {
                $('div#main').addClass('category_wrapper');
            }

            if ('False' == 'True') {
                $('div#main').addClass('landing');
            }

            var getSelectedElement = $('nav.desktop_mega a[href="' + location.href + '"]');
            if (getSelectedElement.length == 0) {
                getSelectedElement = $('nav.desktop_mega a[href="' + document.referrer + '"]');
            }

            if (window.location.pathname != '/' && window.location.pathname.toLowerCase().indexOf('/product/main.aspx') < 0 && window.location.pathname.toLowerCase().indexOf('/default.aspx') < 0) {
                getSelectedElement.parents().each(function () {
                    if ($(this).is('nav.desktop_mega li'))
                        $(this).addClass('selected');
                });
            }
        });

        $('html').click(function () {
            var rightValue = '';

            if ($("#popUp").length) {
                rightValue = $("#popUp").css("right");
            }

            if ($("#popUp_marketing").length) {
                rightValue = $("#popUp_marketing").css("right");
            }

            if (rightValue == '152px') {
                closePopup_SliderBanner();

                if ($("#popUp").length) {
                    if (getSecondCookie("slider_w") == "S1") {
                        $("#popUp").css("display", "none");
                    }
                }
            }
        });

