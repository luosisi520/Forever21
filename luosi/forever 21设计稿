    <script type="text/javascript">
        checkloginlink();
        checkLastCategoryPage();

        $(window).load(function () {

            var wishlistdata = getCookie("WishListData");

            if (wishlistdata != "") {
                var wobj = $.parseJSON(wishlistdata);

                //Update top area wishlist count
                $('.wishlist_count').html(wobj.length);
                if ($('.wishlist_count').html() == 0) {
                    $('.wishlist_count').css('display', 'none');
                }
                else {
                    $('.wishlist_count').css('display', 'block');
                }
            }

        });

    </script>
