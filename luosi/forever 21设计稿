    var AppPath = '';
    var ImageRootPath = 'http://www.forever21.cn/images/';
    var SiteImagePath = 'http://www.forever21.cn/images/en';

    var MSG_OOS = "我们很抱歉,此产品不再可用.";
    var MSG_ERR = "抱歉 ,此商品暂时无法购买";
    var MSG_LIMITOVER = "Your bag has reached the maximum capacity (40 items) for this transaction.";

    function fnHideImageButton(id) {
        document.getElementById(id).style.display = 'none';
    }

    function fnChangeImage(imageClass, newImageURL) {
        $('.ItemImage.Main').fadeOut('fast', function () {
            $('.ItemImage.Main').attr('src', newImageURL);
            $('.ItemImage.Main').fadeIn('fast');
        });
    }

    function fnChangeColor(category, id, colorId, colorName) {
        var url = '/Ajax/Ajax_Product.aspx?method=CHANGEPRODUCTCOLOR&category=' + category + '&productid=' + id + '&colorid=' + colorId;

        $.ajax({
            type: "GET",
            url: url,
            success: function (result) {
                $('.ItemImage.Main').fadeOut('fast', function () {
                    $('.ItemImage.Main').attr('src', result.ProductDefaultImageURL);
                    $('.ItemImage.Main').fadeIn('fast');
                });

                $('#pdp_thumbnail').fadeOut('fast', function () {
                    $('#pdp_thumbnail').html(result.ProductButtonImageHTML);
                    $('#pdp_thumbnail').fadeIn('fast');
                });

                $('#ulProductSize').fadeOut('fast', function () {
                    $('#ulProductSize').html(result.ProductSizeHTML);
                    $('#ulProductSize').fadeIn('fast');
                });

                $('#spanSelectedColorName').fadeOut('fast', function () {
                    $('#spanSelectedColorName').html(colorName);
                    $('#spanSelectedColorName').fadeIn('fast');
                });

                $('#ulProductColor li').removeClass('selected');
                $('#colorid_' + colorId).addClass('selected');
                $('#ulProductSize input:checkbox:checked').prop('checked', false);

                // set selected color value to hidden control
                $('.hdSelectedColor').val(colorId);
                $('.hdSelectedColorName').val(colorName);
                $('.hdSelectedSize').val(result.SelectedSizeID);

                fnChangeSizePDP();
            }
        });
    }

    function fnChangeSize(id) {
        if ($('#' + id).closest('li').find('div.tooltip').length > 0) {
            // the size is out of stock
            $('#' + id).closest('li').find('div.tooltip').css('display', 'block'); // show oos message tooltip
            $('#hdSelectedSizeName_OOS').val($('#' + id).closest('li').find('label').html()); // set size name to oos control
        }
        else {
            fnHideTooltip();
            $('#ulProductSize input:checkbox:checked').prop('checked', false);
            $('#' + id).prop('checked', true);
            $('.hdSelectedSize').val($('#' + id).val());

            fnChangeSizePDP();
        }
    }

    function fnChangeQty(qty) {
        $('.hdSelectedQty').val(qty);
    }

    function fnCart() {
        var brandVal = $('.hdBrand').val();
        var siteNameVal = $('.hdCatalogName').val();
        var categoryVal = $('.hdCategoryName').val();
        var productIdVal = $('.hdProductId').val();
        var itemCodeVal = $('.hdItemCode').val();
        var displayNameVal = $('.hdDisplayName').val();
        var itemPriceVal = $('.hdItemPrice').val();

        var selectedColorVal = $('.hdSelectedColor').val();
        var selectedColorNameVal = $('.hdSelectedColorName').val();
        var selectedSizeVal = $('.hdSelectedSize').val();
        var selectedQtyVal = $('.hdSelectedQty').val();

        var imgURL = $('.ItemImage.Main').attr('src').replace('/default_750/', '/thumbnail/');

        if (brandVal == "tbt" || brandVal == "21men")
            brandVal = "f21";

        if (selectedColorVal == "") {
            $('.tooltip_color').css('display', 'block');
            return false;
        }
        if (selectedSizeVal == "") {
            $('.tooltip_size').css('display', 'block');
            return false;
        }

        $('#addtobag').html('adding...');

        OpacityInit('showBasket');

        var VariantID = selectedColorVal + selectedSizeVal;

        var url = AppPath + "/Ajax/AjaxBasket.aspx?Action=AddProduct&Category=" + categoryVal + "&ProductID=" + productIdVal + "&Catalog=" + siteNameVal + "&Quantity=" + selectedQtyVal + "&VariantID=" + VariantID + "&BR=" + brandVal + "&" + new Date().getTime();
        var br_sku = itemCodeVal + VariantID;

        try {
            BrTrk.getTracker().logEvent('Cart', 'click-add', { 'prod_id': productIdVal, 'sku': itemCodeVal + VariantID, 'prod_name': displayNameVal }, { 'price': itemPriceVal });
        }
        catch (err)
        { }

        $.ajax({
            type: "GET",
            url: url,
            success: function (result) {
                fn_AddToBasket(result, imgURL, 'showBasket', brandVal, selectedColorVal);

                $('#addtobag').html('added!');

                // certona
                //var resx = new Object();
                resx.appid = "FOREVER2101";
                resx.top1 = 100000;
                resx.top2 = 100000;

                resx.event = "shopping+cart";
                resx.itemid = productIdVal;
                resx.customerid = certonaCustomerID;

                certonaResx.run();

                setTimeout(function () { $('#addtobag').html('add to bag'); }, 2000);
            }
        });

        return false;
    }

    function fnAddWishList() {
        var returnValue = true;

        var Qty = '';
        var Color = '';
        var Size = '';

        Qty = $('.hdSelectedQty').val();
        Color = $('.hdSelectedColor').val();
        Size = $('.hdSelectedSize').val();

        var arrColor, VariantID;

        if (Color != null)
            arrColor = Color.split("|");

        if (Color == "") {
            alert("请选颜色");
            returnValue = false;
        }

        if (Size == "") {
            $('.tooltip_size').css('display', 'block');
            returnValue = false;
        }

        return returnValue;
    }

    function fnShowSizechart(brand, category) {
        var sizeChartPageURL = '';

        if (brand == 'love21') {
            sizeChartPageURL = '/htmls/sizechart/m_sizecontemp.html';
        }
        else if (brand == '21men') {
            sizeChartPageURL = '/htmls/sizechart/m_sizemen.html';
        }
        else if (brand == 'plus') {
            sizeChartPageURL = '/htmls/sizechart/m_sizeplus.html';
        }
        else if (brand == 'girls' || brand == 'boys' || brand == 'kids') {
            sizeChartPageURL = '/htmls/sizechart/m_sizekids.html';
        }
        else if (category.toLowerCase().indexOf('acc') >= 0) {
            sizeChartPageURL = '/htmls/sizechart/m_sizeacc.html';
        }
        else {
            sizeChartPageURL = '/htmls/sizechart/m_sizewomen.html';
        }

        $.confirm({
            title: '',
            content: 'url:' + sizeChartPageURL,
            animation: 'top',
            closeIcon: true,
            confirmButton: false,
            cancelButton: false,
            columnClass: 'col-md-6'
        });
    }

    function fnShowWaitlist(producttype, productid, sizeid) {
        $('#hdProductType_OOS').val(producttype);
        $('#hdProductID_OOS').val(productid);
        $('#hdSelectedSizeID_OOS').val(sizeid);

        $.confirm({
            title: '',
            content: 'url:/Product/Product_Waitlist_pop.aspx',
            animation: 'top',
            closeIcon: true,
            confirmButton: false,
            cancelButton: false,
            columnClass: 'col-md-4'
        });
    }

    function RemainTimer() {
        var hours = difference.get('hours');
        if (hours < 10) {
            hours = "0" + hours;
        }
        var minutes = difference.get('minutes');
        if (minutes < 10) {
            minutes = "0" + minutes;
        }

        var seconds = difference.get('seconds');
        if (seconds < 10) {
            seconds = "0" + seconds;
        }

        $('.span_remain_time').html("ends in " + hours + ":" + minutes + ":" + seconds);

        if (difference < 1) {
            clearInterval(countdownTimer);
            $('.span_remain_time').html('Completed');
        } else {
            difference.add(-1000);
        }
    }

    $(window).load(function () {
        var now = moment();
        tomorrow = moment().add(1, 'day').startOf('day');
        difference = moment.duration(tomorrow.diff(now));


        countdownTimer = setInterval('RemainTimer()', 1000);
    });
</script>
