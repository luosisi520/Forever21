<script type="text/javascript">
        function emailPopupVisible() {
        var ncPop = GetCookie('NCPOP');

        //탭 팝업
        if (ncPop == null) {
            ncPop = 1
            setCookie('NCPOP', ncPop, 7);
            $('.signup_enjoy').show();
            $('#email_result').hide();

        } else if ('true' == 'True' || 'True' == 'True') {
            $('.signup_enjoy').show();
            $('#email_result').hide();   
        }
        else {
            $('.signup_enjoy').hide();
        }
    }

    $(document).ready(function () {
        emailPopupVisible();
    });

