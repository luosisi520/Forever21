<script type="text/javascript">
    function closePop_SignUp() {
        $('.signup_enjoy').hide();
    }
    function EmailSubscribe(selector, type) {
        if ($(selector).val() == '' || (/^[_0-9a-zA-Z-]+(\.[_0-9a-zA-Z-]+)*@[0-9a-zA-Z-]+(\.)+([0-9a-zA-Z-]+)([\.0-9a-zA-Z-])*$/).test($(selector).val()) == false) {
            alert("请确认您的邮件。");
            $(selector).focus();
            return false;
        }
        else {
            document.getElementById("pcEmailOptin").disabled = true;
            SubScribeEmail(selector, "/Controls/Subscribe/EmailSubscribe.aspx", "forever21");
            return;
        }
    }
    //PopUp 설정
    function SubScribeEmail(selector, url, emailType) {
        var email = $(selector).val();
        var nowTime = '2018-12-07 8:23:10';
        $.ajax({
            type: "POST",
            url: url,
            data: {
                email: email,
                subTime: nowTime,
                emailType: emailType
            },
            success: function (sResultText) {
                if (sResultText == "OK") {
                    $(selector).val('');
                    $('.signup_enjoy').hide();
                    $('#email_result').show();
                } else {
                    alert(sResultText);
                    $(selector).focus();
                }
                document.getElementById("pcEmailOptin").disabled = false;
                return;
            },
            error: function () {
                alert("发生错误信息，请重试");
                $(selector).focus();
                document.getElementById("pcEmailOptin").disabled = false;
                return;
            }
        });
    }
</script>
<!-- emailOptin:등록 -->

<!-- e:등록 -->

